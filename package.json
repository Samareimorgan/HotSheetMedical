{
  "name": "hotsheetmedical",
  "version": "0.1.0",
  "engines": {
    "node": "8.11.3"
  },
  "private": true,
  "dependencies": {
    "@babel/core": "^7.0.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-router": "^0.0.1",
    "http-proxy-middleware": "^0.19.1",
    "if-env": "^1.0.4",
    "jsesc": "^2.5.1",
    "mongoose": "^5.4.1",
    "morgan": "^1.9.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-html-parser": "^2.0.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.2",
    "react-tooltip": "^3.9.2",
    "reactstrap": "^6.5.0",
    "unirest": "^0.5.1",
    "webpack": "^4.28.3"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "npm run client",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
